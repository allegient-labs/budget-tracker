<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Context>
<Context cookies="true">

  <Resource
    name="jdbc/project"
    username="project"
    password="project"
    auth="Container"
    driverClassName="oracle.jdbc.OracleDriver"
    url="jdbc:postgresql:project"
    maxActive="10"
    maxIdle="5"
    maxWait="10000"
    type="javax.sql.DataSource" />
    
</Context>
